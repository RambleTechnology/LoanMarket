    <script type="text/javascript">
        var banner_swiper = new Swiper('.banner .swiper-container', {
            pagination: '.banner .swiper-pagination',
            autoplay: 3000
        });

        (() => {
            var n = 1
            setInterval(() => {
                let topHeight = '-' + n * 50 + 'px'
                $(".newsWrapper").animate({ top: topHeight }, 200)
                n === $('.newsWrapper').children('li').length - 1 ? n = 0 : n++
            }, 3000)
        })()
        var copybtn = null;
        var clipboard = null;

        init();
        function init() {                    
            //设置title
            $.ajax({
                url: "/api/ProductType",
                data: { no: 7 },
                type: 'Get',
                success: function (data) {
                    if (data) {
                        $(".productTypeName").html(data.Name);
                    }
                },
                error: function (e) {
                    console.error("服务器异常" + e);
                }
            });


            //获取该类别的子类别数据
            $.ajax({
                url: "/api/TypeViewGrade",
                data: { id: 7 },
                type: 'Get',
                success: function (data) {
                    var html = "";
                    if (data) {
                        for (var i = 0; i < data.length; i++) {
                            html += "<li>";
                            html += "<a href='/Product/ProductList?typeId=" + data[i].No + "'>";
                            html += "<img src='" + data[i].Icon + "'><span class='txt' style='width: 100px;'>" + data[i].Name + "</span>";
                            html += "</a>";
                            html += "</li>";
                        }
                    } else {
                        html += "<li>";
                        html += "无数据";
                        html += "</li>";
                    }
                    $(".data").html(html);
                },
                error: function (e) {
                    console.error("服务器异常" + e);
                }
            });
        }
        function getqrcode(id) {
            $.ajax({
                url: "/index/member/getqrcode.html",
                data: { id: id },
                type: 'post',
                success: function (res) {
                    if (res.code == 1) {
                        $(".shadow-box").html(res.msg.html);
                        copybtn = document.getElementById('copy');
                        clipboard = new ClipboardJS(copybtn, {
                            text: function () {
                                return res.msg.url;
                            }
                        });
                        $(".apply-shadow").show();
                    } else {
                        if (res.msg == 'noaccess') {
                            location.href = "/index/Index/noaccess";
                        } else if (res.msg == 'document') {
                            location.href = "/index/Api/gatherpage?id=" + id;
                        } else {
                            alerterr(res.msg);
                        }
                    }
                }
            })
        }

        $(".shadow-box").on('click', '.closebox', function () {
            $('.apply-shadow').hide();
            $(".shadow-box").html('');
        })

        function copy(url) {
            clipboard.on('success', function (e) {
                mui('#browser').popover('hide');
                alertok('复制成功');
            });
            clipboard.on('error', function (e) {
                alerterr('复制失败');
            });
        }

        function jump(url) {
            location.href = url;
        }

        function feedback(id, classname) {
            $.ajax({
                url: "/index/index/feedback.html",
                data: { id: id },
                type: 'post',
                success: function (res) {
                    if (res.code == 1) {
                        $("." + classname).attr('disabled', true);
                        $("." + classname).text('已反馈');
                    } else {
                        alerterr(res.msg);
                    }
                }
            })
        }


    </script>




