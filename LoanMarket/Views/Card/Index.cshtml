    <script type="text/javascript">
        $("#moreMessage").on("click", function () {
            $(".bgMessage").show();
        })
        $(".closeMessage").on("click", function () {
            $(".bgMessage").hide();
        })
        var banner_swiper = new Swiper('.banner .swiper-container', {
            pagination: '.banner .swiper-pagination',
            autoplay: 3000
        });

        (() => {
            var n = 1
            setInterval(() => {
                let topHeight = '-' + n * 50 + 'px'
                $(".newsWrapper").animate({ top: topHeight }, 200)
                n === $('.newsWrapper').children('li').length - 1 ? n = 0 : n++
            }, 3000)
        })()
        var copybtn = null;
        var clipboard = null;

        init();
        function init() {

            //获取37类别下面的产品
            $.ajax({
                url: "/api/Product",
                data: { typeId: 37 },
                type: 'Get',
                success: function (data) {
                    var html = "";
                    if (data) {
                        for (var i = 0; i < data.length; i++) {
                            html += "<li style='width:24.33333%'>";
                            html += "<a onclick='GetDetail(" + data[i].No + ")'>";
                            html += "<img src='" + data[i].Icon + "'><span class='txt' style='width: 100px;'>" + data[i].Name + "</span><span style='color:red'>" + data[i].Describe5 + "</span>";
                            html += "</a>";
                            html += "</li>";
                        }
                    } else {
                        html += "<li>";
                        html += "无数据";
                        html += "</li>";
                    }
                    $(".data34").html(html);
                },
                error: function (e) {
                    console.error("服务器异常" + e);
                }
            });

            //获取8类别下面的产品
            $.ajax({
                url: "/api/Product",
                data: { typeId: 8 },
                type: 'Get',
                success: function (data) {
                    var html = "";
                    if (data) {
                        for (var i = 0; i < data.length; i++) {
                            html += "<li style='width:24.33333%'>";
                            html += "<a onclick='GetDetail(" + data[i].No + ")'>";
                            html += "<img src='" + data[i].Icon + "'><span class='txt' style='width: 100px;'>" + data[i].Name + "</span>";
                            html += "</a>";
                            html += "</li>";
                        }
                    } else {
                        html += "<li>";
                        html += "无数据";
                        html += "</li>";
                    }
                    $(".data35").html(html);
                },
                error: function (e) {
                    console.error("服务器异常" + e);
                }
            });
        }


        $(".shadow-box").on('click', '.closebox', function () {
            $('.apply-shadow').hide();

        })




        //获取产品详情
        function GetDetail(no) {
            $.ajax({
                url: "/api/ProductDetail",
                data: { no: no },
                type: 'Get',
                success: function (data) {
                    var type = $(".productTypeName").html();
                    $(".detail .TypeAndName").html(type + data.Name);
                    $(" .detail  .Describe").html(data.Describe1 + "</br>" + data.Describe2 + "</br>" + data.Describe3 + "</br>" + data.Describe4 + "</br>" + data.Describe5 + "</br>");
                    $(".detail .Icon").attr("src", data.Icon);
                    $(".detail .Name").html(data.Name);
                    $(".detail  .QrCode").attr("src", qrCodeRootPath + data.QrCodeUrl);
                    $(".detail .Url").attr("data-url", data.Url);
                    $(".detail .JumpUrl").attr("href", data.Url);
                    $(".detail .saveImg").attr("href", "/Product/SaveImg?no=" + data.No);
                    $(".apply-shadow").show();
                },
                error: function () {
                    console.log("服务器异常");
                }
            });
        }



