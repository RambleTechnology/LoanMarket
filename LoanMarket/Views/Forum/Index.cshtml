            $("body").on("click", ".fabulous", function () {
                var id = $(this).attr('data-id');
                var that = $(this);
                $.ajax({
                    url: "/index/forum/fabulous.html?id=" + id,
                    dataType: 'json',
                    success: function (res) {
                        if (res.code == 1) {
                            that.removeClass('fabulous');
                            that.addClass('unfabulous');
                            that.addClass('color-red');
                            var fabulous = parseInt(that.children('font').text());
                            fabulous += 1;
                            that.children('font').text(fabulous);
                        } else {
                            alerterr(res.msg);
                        }
                    }
                })
            })

            $("body").on("click", ".unfabulous", function () {
                var id = $(this).attr('data-id');
                var that = $(this);
                $.ajax({
                    url: "/index/forum/unfabulous.html?id=" + id,
                    dataType: 'json',
                    success: function (res) {
                        if (res.code == 1) {
                            that.removeClass('unfabulous');
                            that.addClass('fabulous');
                            that.removeClass('color-red');
                            var fabulous = parseInt(that.children('font').text());
                            fabulous -= 1;
                            that.children('font').text(fabulous);
                        } else {
                            alerterr(res.msg);
                        }
                    }
                })
            })

        });
