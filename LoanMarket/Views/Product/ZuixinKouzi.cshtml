    <input type="hidden" value="@Model" class="typeId" />

    <script type="text/javascript">
      
        var copybtn = null;
        var clipboard = null;
        init();
        function init() {
            //获取类别编号
            var typeId = $(".typeId").val();

            //设置title
            $.ajax({
                url: "/api/ProductType",
                data: { no: typeId },
                type: 'Get',
                success: function (data) {
                    if (data) {
                        $(".productTypeName").html(data.Name);
                    }
                },
                error: function (e) {
                    console.error("服务器异常" + e);
                }
            });

            //获取该类别的数据
            $.ajax({
                url: "/api/Product",
                data: { typeId: typeId },
                type: 'Get',
                success: function (data) {
                    var html = "";
                    if (data) {
                        for (var i = 0; i < data.length; i++) {
                            html += "<li>";
                            html += "<a onclick='GetDetail(" + data[i].No + ")'>";
                            html += "<img src='" + data[i].Icon + "'><span class='txt' style='width: 100px;'>" + data[i].Name + "</span>";
                            html += "</a>";
                            html += "</li>";
                        }
                    } else {
                        html += "<li>";
                        html += "无数据";
                        html += "</li>";
                    }
                    $(".data").html(html);
                },
                error: function (e) {
                    console.error("服务器异常" + e);
                }
            });
        }

        //获取详情
        function getqrcode(id) {
            $.ajax({
                url: "/index/member/getqrcode.html",
                data: { id: id },
                type: 'post',
                success: function (res) {
                    if (res.code == 1) {
                        $(".shadow-box").html(res.msg.html);
                        copybtn = document.getElementById('copy');
                        clipboard = new ClipboardJS(copybtn, {
                            text: function () {
                                return res.msg.url;
                            }
                        });
                        $(".apply-shadow").show();
                    } else {
                        if (res.msg == 'noaccess') {
                            location.href = "/index/Index/noaccess";
                        } else if (res.msg == 'document') {
                            location.href = "/index/Api/gatherpage?id=" + id;
                        } else {
                            alerterr(res.msg);
                        }
                    }
                }
            })
        }

        //获取产品详情
        function GetDetail(no) {
            $.ajax({
                url: "/api/ProductDetail",
                data: { no: no },
                type: 'Get',
                success: function (data) {
                    var type = $(".productTypeName").html();
                    $(".detail .TypeAndName").html(type + data.Name);
                    $(" .detail  .Describe").html(data.Describe1 + "</br>" + data.Describe2 + "</br>" + data.Describe3 + "</br>" + data.Describe4 + "</br>" + data.Describe5 + "</br>");
                    $(".detail .Icon").attr("src", data.Icon);
                    $(".detail .Name").html(data.Name);
                    $(".detail  .QrCode").attr("src", qrCodeRootPath + data.QrCodeUrl);
                    $(".detail .Url").attr("data-url", data.Url);
                    $(".detail .JumpUrl").attr("href", data.Url);
                    $(".apply-shadow").show();
                },
                error: function () {
                    console.log("服务器异常");
                }
            });
        }

        //关闭详情
        $(".shadow-box").on('click', '.closebox', function () {
            $('.apply-shadow').hide();
            //$(".shadow-box").html('');
        })

