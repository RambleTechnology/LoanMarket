                alertok('复制成功');
            });
            clipboard.on('error', function (e) {
                alerterr('复制失败');
            });
        }

        function jump(url) {
            location.href = url;
        }

        function feedback(id, classname) {
            $.ajax({
                url: "/index/index/feedback.html",
                data: { id: id },
                type: 'post',
                success: function (res) {
                    if (res.code == 1) {
                        $("." + classname).attr('disabled', true);
                        $("." + classname).text('已反馈');
                    } else {
                        alerterr(res.msg);
                    }
                }
            })
        }
