            //获取该类别的子类别数据
            $.ajax({
                url: "/api/TypeViewGrade",
                data: { id: 6 },
                type: 'Get',
                success: function (data) {
                    var html = "";
                    if (data) {
                        for (var i = 0; i < data.length; i++) {
                            html += "<li>";
                            html += "<a href='/Product/ProductList?typeId=" + data[i].No + "'>";
                            html += "<img src='" + data[i].Icon + "'><span class='txt' style='width: 100px;'>" + data[i].Name + "</span>";
                            html += "</a>";
                            html += "</li>";
                        }
                    } else {
                        html += "<li>";
                        html += "无数据";
                        html += "</li>";
                    }
                    $(".data").html(html);
                },
                error: function (e) {
                    console.error("服务器异常" + e);
                }
            });
        }
        function getqrcode(id) {
            $.ajax({
                url: "/index/member/getqrcode.html",
                data: { id: id },
                type: 'post',
                success: function (res) {
                    if (res.code == 1) {
                        $(".shadow-box").html(res.msg.html);
                        copybtn = document.getElementById('copy');
                        clipboard = new ClipboardJS(copybtn, {
                            text: function () {
                                return res.msg.url;
                            }
                        });
                        $(".apply-shadow").show();
                    } else {
                        if (res.msg == 'noaccess') {
                            location.href = "/index/Index/noaccess";
                        } else if (res.msg == 'document') {
                            location.href = "/index/Api/gatherpage?id=" + id;
                        } else {
                            alerterr(res.msg);
                        }
                    }
                }
            })
        }

        $(".shadow-box").on('click', '.closebox', function () {
            $('.apply-shadow').hide();
            $(".shadow-box").html('');
        })

        function copy(url) {
            clipboard.on('success', function (e) {
